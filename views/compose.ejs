<%- include("partials/header"); -%>
<h1>Compose</h1>
<form class="form-group" action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input type="text" class="form-control" name="postTitle" id="postTitle" />
    <label for="postText">Post</label>
    <textarea class="form-control" id="postText" rows="5"></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer"); -%>
